{
  "name": "Notes",
  "short_name": "Notes",
  "theme_color": "#ffffff",
  "background_color": "#ffffff",
  "display-override": ["browser", "fullscreen", "standalone", "minimal-ui"],
  "display": "minimal-ui",
  "orientation": "portrait",
  "description": "Notes — это приложение для создания заметок",
  "scope": "/",
  "protocol_handlers": [
    {
      "protocol": "web+tea",
      "url": "/tea?type=%s"
    },
    {
      "protocol": "web+coffee",
      "url": "/coffee?type=%s"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/screenshot-small.png",
      "type": "image/png",
      "sizes": "1000x1560",
      "form_factor": "narrow"
    },
    {
      "src": "/screenshots/screenshot-large.png",
      "type": "image/png",
      "sizes": "3024x1964",
      "form_factor": "wide"
    }
  ],
  "start_url": "/",
  "icons": [
    {
      "src": "./icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "./icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
